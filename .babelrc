{
    "presets": [
        [
            "@babel/env",
            {
                "useBuiltIns": "entry",
                "targets": {
                    "browsers": "> 1%, not ie 11, not op_mini all",
                    "node": 14
                }
            }
        ],
        "@babel/react"
    ],

    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/syntax-dynamic-import",
        "@babel/transform-runtime",
        "@loadable/babel-plugin",
        "emotion",
        [
            "import",
            {
                "libraryName": "@material-ui/icons",
                "libraryDirectory": "",
                "camel2DashComponentName": false
            }
        ]
    ]
}
