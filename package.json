{
    "name": "my-working-time",
    "version": "0.1.0",
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "start": "node dist/main.js",
        "watch:server": "webpack --mode development --watch",
        "watch:build": "nodemon dist/main.js --watch public",
        "watch": "npm-run-all --parallel watch:server watch:build",
        "dev": "rd /q /s public && rd /q /s dist && webpack --mode development --progress",
        "build": "webpack --mode production --progress",
        "stats": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist"
    },
    "dependencies": {
        "@date-io/moment": "1.x",
        "@emotion/core": "^10.0.28",
        "@loadable/component": "^5.13.1",
        "@loadable/server": "^5.13.1",
        "@material-ui/core": "^5.0.0-alpha.5",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/pickers": "^3.2.10",
        "axios": "^0.19.2",
        "express": "^4.17.1",
        "history": "4.7.2",
        "immer": "^7.0.5",
        "lodash.isequal": "^4.5.0",
        "moment": "^2.27.0",
        "normalizr": "^3.6.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-jss": "^10.3.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "react-spinners": "^0.9.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@loadable/babel-plugin": "^5.13.0",
        "@loadable/webpack-plugin": "^5.13.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-import": "^1.13.0",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^4.2.0",
        "dotenv": "^8.2.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "npm-run-all": "^4.1.5",
        "react-loadable": "^5.5.0",
        "reload": "^3.1.0",
        "webpack": "^4.44.1",
